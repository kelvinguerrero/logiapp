<div class="col-lg-12 well well-sm">
  <fieldset>
      <div class="form-group">
        <%= f.label 'container' %><br>
        <%= f.text_area :container, :class => 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label 'description' %><br>
        <%= f.text_area :description, :class => 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label 'route' %><br>
        <%= f.text_area :route, :class => 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label 'Total' %><br>
        <%= f.number_field :total, :class => 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.check_box :_destroy %>
        <%= f.label 'Eliminar'%>
      </div>

      <%= f.hidden_field :_destroy %>
      <%= link_to "remove", '#', class: "remove_fields" %>
      <br>
  </fieldset>
</div>
