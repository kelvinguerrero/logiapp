<h1>Listing type_movements</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_movements.each do |type_movement| %>
      <tr>
        <td><%= type_movement.name %></td>
        <td><%= link_to 'Show', type_movement %></td>
        <td><%= link_to 'Edit', edit_type_movement_path(type_movement) %></td>
        <td><%= link_to 'Destroy', type_movement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type movement', new_type_movement_path %>
